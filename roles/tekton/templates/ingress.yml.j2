---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: tekton-ingress
spec:
  ingressClassName: nginx
  tls:
  rules:
  - host: tekton.ilkerispir.com
    http:
      paths:
      - path: /
        pathType: Exact
        backend:
          service:
            name: tekton-dashboard
            port:
              number: 9097
